<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Name="CommandTextBox" Width="360" Height="30" VerticalAlignment="Top" Margin="0,10,0,0" />
        <Button Name="ProcessButton" Content="Process" Width="100" Height="30" VerticalAlignment="Top" Margin="0,50,0,0" Click="ProcessButton_Click" />
        <TextBlock Name="ResultTextBlock" Width="360" Height="80" VerticalAlignment="Top" Margin="0,90,0,0" TextWrapping="Wrap" />
    </Grid>
</Window>
